#!/bin/sh


export TARGET_NAME=ruboto
export BRANCH_NAME=Linux
export SERVER_NAME="localhost"

export ANDROID_HOME="/work/bin/Linux/android_sdk"
export PATH=${ANDROID_HOME}/tools:${PATH}

export PRJ_DIR=/work/src/ruboto
export SRC_DIR=/work/src/ruboto
export ENV_FILE=/work/bin/env_${TARGET_NAME}


alias src='cd ${SRC_DIR}'
alias prj='cd ${PRJ_DIR}'
alias doc='cd /work/books/ruby'


build () {
    cd ${SRC_DIR}
}




