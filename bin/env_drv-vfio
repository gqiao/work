#!/bin/sh


export TARGET_NAME=x86_64
export BRANCH_NAME=drv-vfio
export SERVER_NAME="github.com"
export SSH_AUTH_SOCK=0

export KERNEL_DIR=/work/src/kvm/linux-vfio
export SRC_DIR=/work/src/drv/drv-vfio
export PRJ_DIR=/work/prj/drv-vfio
export ENV_FILE=/work/bin/env_${BRANCH_NAME}
export ARCH=x86_64


alias src='cd ${SRC_DIR}'
alias prj='cd ${PRJ_DIR}'
alias doc='cd /work/doc'

build () {
    cd ${SRC_DIR}
    make
}



cd ${PRJ_DIR}

